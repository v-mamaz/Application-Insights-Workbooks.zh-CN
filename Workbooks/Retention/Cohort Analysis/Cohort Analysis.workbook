                        "name": "CohortType",
                        "type": 2,
                        "description": null,
                        "isRequired": true,
                        "multiSelect": null,
                        "query": "datatable(value:string, text:string)[\n'itemType', 'âŒ None',\n'name', 'ğŸ“› Activity',\n'cloud_RoleInstance', 'ğŸ–¥ï¸ Machine',\n'client_CountryOrRegion', 'ğŸ“ Country or Region',\n'client_StateOrProvince', 'ğŸ“ State or Province',\n'client_City', 'ğŸ“ City',\n'client_Browser', 'ğŸŒ Client Browser',\n'client_OS', 'ï¸ï¸ğŸ–¥ï¸ Client Operating System',\n'client_Model', 'ğŸ“± Client Model',\n'application_Version', 'âšª Application Version',\n'operation_Name', 'âšª Operation',\n]\n| union (union pageViews, customEvents\n| where timestamp >= ago(1d)\n| where name in ({CohortingActivities}) or '*' in ({CohortingActivities})\n| project customDimensions \n| summarize schema = buildschema(customDimensions) \n| mvexpand schema\n| extend e = extract(@'{\"(.+)\":.+}', 1, tostring(schema))\n| project text = strcat('â„ï¸ ', e), value = strcat('customDimensions[\"', e, '\"]'))",
