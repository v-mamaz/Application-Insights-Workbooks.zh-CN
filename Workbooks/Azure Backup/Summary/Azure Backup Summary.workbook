                        "version": "KqlParameterItem/1.0",
                        "name": "BackupItemPageNumber",
                        "label": "Select Page Number",
                        "type": 2,
                        "description": "<span data-ttu-id=\"56502-108\">选择页码以查看更多记录</span><span class=\"sxs-lookup\"><span data-stu-id=\"56502-108\">Select Page Number to view more records</span></span>",
