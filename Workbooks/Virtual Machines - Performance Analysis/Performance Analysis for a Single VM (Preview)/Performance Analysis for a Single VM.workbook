                        "name": "CounterText",
                        "type": 1,
                        "query": "let metric = dynamic({Counter});\r\nrange Steps from 1 to 1 step 1\r\n| project strcat(metric.object, \" > \", metric.counter)",
                        "crossComponentResources": [
