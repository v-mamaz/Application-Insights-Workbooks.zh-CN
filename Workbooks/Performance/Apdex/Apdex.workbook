                        "type": 2,
                        "isRequired": true,
                        "query": "datatable(value:string, text:string)[\r\n'name', 'âšª Activity Name',\r\n'cloud_RoleInstance', 'ğŸ–¥ï¸ Machine',\r\n'client_CountryOrRegion', 'ğŸ“ Country or Region',\r\n'client_StateOrProvince', 'ğŸ“ State or Province',\r\n'application_Version', 'âšª Application Version',\r\n'operation_Name', 'âšª Operation',\r\n]\r\n| union ({Type}\r\n| where timestamp {TimeRange}\r\n| where name in ({Operations}) or '*' in ({Operations})\r\n| project customDimensions \r\n| summarize schema = buildschema(customDimensions) \r\n| mvexpand schema\r\n| extend e = extract(@'{\"(.+)\":.+}', 1, tostring(schema))\r\n| project text = strcat('â„ï¸ ', e), value = strcat('customDimensions[\"', e, '\"]'))",
